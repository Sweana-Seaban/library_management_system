{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  // "extends": "eslint:recommended",
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "prettier/prettier": "warn",
    "no-unused-vars": "warn",
    "indent": ["error", "tab"],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "no-undef": "off"
  }
}
